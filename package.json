{
  "name": "easy-blob",
  "version": "1.0.0",
  "description": "Quick and easy local blob storage",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "echo 'This is a library package. See README for usage examples.'",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "lint:md": "markdownlint *.md",
    "lint:md:fix": "markdownlint --fix *.md",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "npm run lint:md && npm run format:check"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jesselpalmer/easy-blob"
  },
  "keywords": [
    "blob",
    "storage",
    "file-upload",
    "file-storage",
    "local-storage",
    "express",
    "typescript",
    "sqlite",
    "multer"
  ],
  "author": "Jesse Palmer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jesselpalmer/easy-blob/issues"
  },
  "homepage": "https://github.com/jesselpalmer/easy-blob#readme",
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/supertest": "^2.0.16",
    "jest": "^29.7.0",
    "markdownlint-cli": "^0.38.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.0"
  }
}
